public class DrawImg {
    public static void main(String[] args) {
        int[][] points = new int[3][3];
        for (int i = 0; i < 3; i++) {
            for (int j = 0; j < 3; j++) {
                points[i][j] = i == j ? 1 : 0;
                System.out.print(points[i][j]);
            }
        }
    }

}