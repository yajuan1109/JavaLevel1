public class DrawImg {
    public static void main(String[] args) {
    	int width = 100;
    	int height = 100;
        int[][] points = new int[width][height];
        for (int i = 0; i < width; i++) {
            for (int j = 0; j < height; j++) {
                points[i][j] = i == j ? 1 : 0;
                System.out.print(points[i][j]);
            }
        }
        IfsViewer.view(points);
    }

}